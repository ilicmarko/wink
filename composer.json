{
    "name": "writingink/wink",
    "description": "Wink Publishing Framework.",
    "keywords": [
        "framework",
        "laravel",
        "blog"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Said",
            "email": "mohamed@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3||^7.4",
        "laravel/framework": "^7.0",
        "pbmedia/laravel-ffmpeg": "^6.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^0.6.2"
    },
    "autoload": {
        "psr-4": {
            "Wink\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wink\\WinkServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
